<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sportify.MainController">
   <children>
      <VBox layoutY="5.0" prefHeight="385.0" prefWidth="165.0" spacing="20.0">
         <children>
            <Label text="ID Commande" />
            <Label text="Prix Totale" />
            <Label text="Id user" />
            <Label text="Id panier" />
            <Label />
         </children>
         <padding>
            <Insets left="12.0" top="10.0" />
         </padding>
      </VBox>
      <VBox layoutX="167.0" layoutY="-3.0" prefHeight="400.0" prefWidth="149.0" spacing="12.0">
         <children>
            <TextField fx:id="Id_commande" />
            <TextField fx:id="Prix_totale" />
            <TextField fx:id="Id_user" />
            <TextField fx:id="Id_panier" />
            <Button mnemonicParsing="false" onAction="#enregistrerPdf" text="enregistrer PDF" />
         </children>
         <padding>
            <Insets left="10.0" right="10.0" top="10.0" />
         </padding>
      </VBox>
      <TableView fx:id="Table" layoutX="309.0" onMouseClicked="#setCellValue" prefHeight="514.0" prefWidth="460.0">
        <columns>
          <TableColumn fx:id="Id_com" prefWidth="121.0" text="Id commande" />
          <TableColumn fx:id="PrixTotale" prefWidth="124.0" text="Prix totale" />
            <TableColumn fx:id="IdUser" prefWidth="112.0" text="Id User" />
            <TableColumn fx:id="IdPanier" prefWidth="102.0" text="Id panier" />
        </columns>
      </TableView>
      <Button fx:id="add" layoutX="31.0" layoutY="396.0" mnemonicParsing="false" onAction="#AddCommande" text="Ajouter" />
      <Button layoutX="113.0" layoutY="396.0" mnemonicParsing="false" onAction="#Deletecommande" prefHeight="31.0" prefWidth="90.0" text="supprimer" />
      <Button layoutX="215.0" layoutY="396.0" mnemonicParsing="false" onAction="#UpdateCommande" text="modifier" />
   </children>
</AnchorPane>
